{
  "name": "rudixops_jokes",
  "version": "1.1.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "rename": "rm -rf ./src/data/all.json && mv ./src/data/all.json.real ./src/data/all.json",
    "start": "next start -p $PORT",
    "tbuild": "turbo run build",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "build-local": "run-s clean mvstatic buildnext export restorestatic && cd out && live-server",
    "mvstatic": "mv public public1",
    "restorestatic": "mv public1 public",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "check-types": "tsc --noEmit --pretty",
    "dbmigrate": "rm -rf src/data/all.json && mv src/data/all.json.real src/data/all.json",
    "prepare": "husky install",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "workspaces": [
    "/*"
  ],
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@types/pouchdb": "^6.4.0",
    "axios": "^0.27.2",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "next": "^12.2.0",
    "next-seo": "^5.4.0",
    "next-share": "^0.12.1",
    "pouchdb": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.4",
    "tailwind-scrollbar": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@next/bundle-analyzer": "^12.2.0",
    "@testing-library/react": "^13.3.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.4",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4"
  },
  "author": "Ixartz (https://github.com/ixartz)"
}
